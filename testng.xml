<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Mass Test Suite">

    <parameter name="url" value="https://www.masslottery.com/"></parameter>
    <test name="WIN10TEST" annotations="JDK"
          preserve-order="true">
        <parameter name="browser" value="Firefox"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows"/>
        <classes>
            <class name="MyRunner.TestRunner" />
        </classes>
    </test> <!-- Test -->
    <test name="WIN8TEST" annotations="JDK"
          preserve-order="true">
        <parameter name="browser" value="chrome"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <class name="MyRunner.TestRunner" />
        </classes>
    </test> <!-- Test -->
    <test name="MACTEST" annotations="JDK"
          preserve-order="true">
        <parameter name="browser" value="Microsoft Edge"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <class name="MyRunner.TestRunner" />
        </classes>
    </test> <!-- Test -->

</suite>